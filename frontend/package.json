    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "export": "next build && next export -o out",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky"
